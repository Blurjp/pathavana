# Disable Fast Refresh (Hot Module Replacement)
FAST_REFRESH=false

# Disable React development warnings that might interfere
DISABLE_ESLINT_PLUGIN=true

# Force production-like behavior in development
INLINE_RUNTIME_CHUNK=false
GENERATE_SOURCEMAP=false

# Disable Webpack dev server client injection
WDS_SOCKET_HOST=none
WDS_SOCKET_PATH=none
WDS_SOCKET_PORT=0